{
  "moduleFileExtensions": [
    "js",
    "json",
    "ts"
  ],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "moduleNameMapper": {
    "@notes-app/datamodel/datamodel/(.*)": "<rootDir>/../libs/datamodel/src/$1",
    "@notes-app/datamodel/datamodel": "<rootDir>/../libs/datamodel/src"
  }
}